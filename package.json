{
  "name": "betygfi-nextjs",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "start": "rimraf .next && npm run dev",
    "dev": "NODE_ENV=development next dev -p 7000",
    "deploy": "NODE_ENV=production APP_PORT=7000 next start -p 7000",
    "build": "next build",
    "clean:build": "rimraf .next && npm run build",
    "analyze-build": "cross-env ANALYZE=true next build",
    "build:deploy": "npm run clean:build && npm run deploy",
    "patch:add-meta": "npm version patch && node write-meta.js && git add meta/meta.json && git commit -m \"added meta\" && git push",
    "minor:add-meta": "npm version patch && node write-meta.js && git add meta/meta.json && git commit -m \"added meta\" && git push",
    "eslint": "eslint ./src",
    "prepare": "husky install"
  },
  "dependencies": {
    "@chakra-ui/icon": "3.2.0",
    "@chakra-ui/icons": "2.1.1",
    "@chakra-ui/react": "2.8.0",
    "@reduxjs/toolkit": "1.9.5",
    "@tanstack/react-query": "^5.17.9",
    "axios": "^1.7.7",
    "chakra-dayzed-datepicker": "^0.3.0",
    "cookies-next": "^4.3.0",
    "date-fns": "^3.6.0",
    "dayjs": "^1.11.11",
    "dayzed": "^3.2.3",
    "encoding": "^0.1.13",
    "framer-motion": "^11.1.9",
    "html2canvas": "^1.4.1",
    "lodash": "4.17.21",
    "memory-cache": "0.2.0",
    "millify": "6.1.0",
    "moment": "2.29.4",
    "next": "^14.2.15",
    "next-auth": "^4.24.8",
    "pino-pretty": "^11.2.1",
    "react": "^18",
    "react-ace": "^12.0.0",
    "react-apexcharts": "1.4.1",
    "react-date-range": "^2.0.1",
    "react-dom": "^18",
    "react-icons": "4.11.0",
    "react-redux": "8.1.1",
    "react-slick": "^0.30.2",
    "sass": "1.69.5",
    "slick-carousel": "^1.8.1",
    "viem": "^2.0.3",
    "wagmi": "^2.12.17",
    "webpack": "^5.95.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.5.4",
    "@next/bundle-analyzer": "^14.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "eslint-config-next": "^14.0.4",
    "husky": "^8.0.3",
    "rimraf": "^5.0.5"
  }
}
