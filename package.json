{
  "name": "betygfi-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "start": "rm -rf .next && npm run dev",
    "start:dev": "NODE_ENV=production node_modules/next/dist/bin/next start -p 7000",
    "start:qa": "NODE_ENV=production node_modules/next/dist/bin/next start -p 7000",
    "start:prod": "NODE_ENV=production node_modules/next/dist/bin/next start -p 7000",
    "build:dev": "next build",
    "build:qa": "next build",
    "build:prod": "next build",
    "analyze:dev": "cross-env ANALYZE=true next build",
    "test:build": "npm run build:dev && npm run start:dev",
    "eslint": "eslint ./src",
    "prepare": "husky install"
  },
  "dependencies": {
    "@chakra-ui/icon": "3.2.0",
    "@chakra-ui/icons": "2.1.1",
    "@chakra-ui/react": "2.8.0",
    "@reduxjs/toolkit": "1.9.5",
    "apexcharts": "3.41.1",
    "axios": "1.4.0",
    "cookies-next": "4.1.0",
    "ethers": "5.7.2",
    "lodash": "4.17.21",
    "memory-cache": "0.2.0",
    "millify": "6.1.0",
    "moment": "2.29.4",
    "next": "14.0.2",
    "next-auth": "4.24.5",
    "react": "^18",
    "react-apexcharts": "1.4.1",
    "react-dom": "^18",
    "react-icons": "4.11.0",
    "react-reader": "2.0.4",
    "react-redux": "8.1.1",
    "sass": "1.69.5",
    "wagmi": "1.4.5",
    "webpack": "5.89.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.5.4",
    "@next/bundle-analyzer": "^14.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "eslint-config-next": "^14.0.4",
    "husky": "^8.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run eslint"
    }
  }
}