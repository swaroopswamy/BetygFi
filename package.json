{
  "name": "betygfi-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env.dev next dev",
    "start:dev": "NODE_ENV=production env-cmd -f .env.dev node_modules/next/dist/bin/next start -p 7000",
    "start:qa": "NODE_ENV=production env-cmd -f .env.qa node_modules/next/dist/bin/next start -p 7000",
    "start:prod": "NODE_ENV=production env-cmd -f .env.prod node_modules/next/dist/bin/next start -p 7000",
    "build:dev": "env-cmd -f .env.dev next build",
    "build:qa": "env-cmd -f .env.qa next build",
    "build:prod": "env-cmd -f .env.prod next build",
    "analyze:dev": "cross-env ANALYZE=true next build",
    "eslint": "eslint ./src",
    "prepare": "husky install"
  },
  "dependencies": {
    "@chakra-ui/icon": "^3.2.0",
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/next-js": "^2.1.4",
    "@chakra-ui/react": "^2.8.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/raleway": "^5.0.5",
    "@reduxjs/toolkit": "^1.9.5",
    "@svgr/webpack": "^8.0.1",
    "apexcharts": "^3.41.1",
    "axios": "^1.4.0",
    "chakra-react-select": "^4.7.6",
    "encoding": "^0.1.13",
    "env-cmd": "^10.1.0",
    "eslint-config-next": "^13.5.6",
    "ethers": "^5.7.2",
    "framer-motion": "^10.12.23",
    "is-empty": "^1.2.0",
    "memory-cache": "^0.2.0",
    "millify": "^6.1.0",
    "moment": "^2.29.4",
    "next": "^13.5.6",
    "next-auth": "^4.24.5",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.1",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "react-reader": "^2.0.4",
    "react-redux": "^8.1.1",
    "react-table": "^7.8.0",
    "sass": "^1.63.6",
    "wagmi": "^1.4.5",
    "webpack": "^5.89.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.5.4",
    "@next/bundle-analyzer": "^13.5.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "husky": "^8.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run eslint"
    }
  }
}
